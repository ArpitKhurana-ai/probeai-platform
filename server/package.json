{
  "name": "probeai-server",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "echo 'Building ProbeAI server...' && echo 'Commit:' $(git rev-parse --short HEAD 2>/dev/null || echo 'unknown') && esbuild index.ts --platform=node --packages=external --bundle --format=esm --outfile=index.js --external:./vite.ts --external:../vite.config",
    "start": "node provision_database.js && node index.js",
    "provision": "node provision_database.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "@types/pg": "^8.10.7",
    "drizzle-orm": "^0.33.0",
    "zod": "^3.23.5",
    "vite": "^5.4.10",
    "ws": "^8.14.2",
    "@types/express": "^4.17.17",
    "@types/node": "^20.8.0",
    "openid-client": "^5.6.5",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "express-session": "^1.17.3",
    "connect-pg-simple": "^9.0.1",
    "memoizee": "^0.4.15",
    "@types/passport": "^1.0.12",
    "@types/passport-local": "^1.0.35",
    "@types/express-session": "^1.17.7",
    "@types/memoizee": "^0.4.8",
    "algoliasearch": "^4.20.0",
    "nanoid": "^5.0.4"
  },
  "devDependencies": {
    "esbuild": "^0.19.12"
  }
}